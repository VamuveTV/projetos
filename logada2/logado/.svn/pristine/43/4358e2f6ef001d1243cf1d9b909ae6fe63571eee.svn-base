
body { 

background-color:#F4F4F4; 
font-family:Arial, Helvetica, sans-serif; 
margin-left: 0px; 
margin-top: 0px;
margin-right: 0px; 
margin-bottom: 0px; 
background-attachment:scroll; 

}

ul, menu, dir { 

display: block; 
list-style-type: none; -webkit-margin-before: 1em; -webkit-margin-after: 1em; 
-webkit-margin-start: 0px;- webkit-margin-end: 0px; 
-webkit-padding-start: 0px; 

}

h2 { 

font-size:13px; 
color:#909090; 
font-weight:bold; 
margin-left: 3px; 
width:990px;

}

#topo, #menu { 

background: #ffffff

}

#menu li a { 

color:#5e2954; text-decoration:none; 

}

#menu li a:hover { 

background: #ffffff; 
color:#5e2954; 
text-decoration:underline; 

}

.faces { 

width:990px; 
-webkit-padding-start: 0px;

}

.faces li { 

display:inline; 
float: left; 
padding-right:25px;

}

.imgBtSaibaMaisSentindo { 

float:left;
margin-top:5px;

}

.btsAlterarFontes { 

float:right; 
width:115px; 
height:33px; 
margin-top: -36px; 
margin-right:33px;

}

.btsAlterarFontes li {

display:inline; 
float:right; 
padding-right:5px;

}

.btContinuarSentindo { 

float:right; 
margin-right:30px;

}

#outrasfaces { 

clear: both; 
float: left; 
width: 980px; 
margin-top: 27px;

}

.comentar-sentindo { 

width: 926px; 
height: 242px; 
border:#909090 solid 1px;

}

.limpa { 

clear: both 

}

.estiloSelectPerfil { 

color:#909090; 
width: autopx;

}

.escolherTemaPerfil { 

color:#909090; 
font-size:14px;

}

.escolherTemaPerfil span { 

font-size:13px;

}

#temaCorFonte { 

float:right; 
margin-right: 60px; 
margin-top: 12px;

}

.table {

margin: 8px 0;

}

.table th {

background: #666;
color: #fff;
font-size: 14px;

}

.table td {

font-size: 14px;

}

.pg-ctrl {

font:bold 16px verdana;
color: #0063e3;
text-decoration: none; 
cursor: pointer;    
padding:4px;
margin:0px;
border: 0px;

}

.pg-normal {

font:10px verdana;
color: #333;
text-decoration: none; 
cursor: pointer;    
background-color:#f9f9f9;
padding:3px 6px;
margin:1px;
border: 1px solid #0063e3;

}

.pg-selected {

font:10px verdana;
color: #fff;
text-decoration: none;    
cursor: pointer;
background-color:#0063e3;
padding:3px 6px;
margin:1px;
border: 1px solid #dde;

}

