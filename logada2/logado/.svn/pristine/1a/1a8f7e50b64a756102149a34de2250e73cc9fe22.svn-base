<?php

namespace Atividades\Entity;
use Doctrine\ORM\Mapping as ORM;
/**
 * @ORM\Table(name="atividade")
 * @ORM\Entity */
class Atividade {
    /**
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     * @ORM\Column(type="integer")
     */
    protected $id;

    /** @ORM\Column(type="string") */
    protected $nome;

    /** @ORM\Column(type="string") */
    protected $local;

    /** @ORM\Column(type="string") */
    protected $tipo_destino;

    /** @ORM\Column(type="integer") */
    protected $id_destino;

    /** @ORM\Column(type="string") */
    protected $data;

    /** @ORM\Column(type="string") */
    protected $hora_inicio;

    /** @ORM\Column(type="string") */
    protected $hora_fim;

    /** @ORM\Column(type="string") */
    protected $descricao;

    /** @ORM\Column(type="string") */
    protected $tipo_dono;

    /** @ORM\Column(type="integer") */
    protected $id_dono;

    /** @ORM\Column(type="string") */
    protected $data_criacao;

    /**
     * @param mixed $data_criacao
     */
    public function setDataCriacao($data_criacao)
    {
        $this->data_criacao = $data_criacao;
    }

    /**
     * @return mixed
     */
    public function getDataCriacao()
    {
        return $this->data_criacao;
    }

    /**
     * @param mixed $descricao
     */
    public function setDescricao($descricao)
    {
        $this->descricao = $descricao;
    }

    /**
     * @return mixed
     */
    public function getDescricao()
    {
        return $this->descricao;
    }

    /**
     * @param mixed $hora_fim
     */
    public function setHoraFim($hora_fim)
    {
        $this->hora_fim = $hora_fim;
    }

    /**
     * @return mixed
     */
    public function getHoraFim()
    {
        return $this->hora_fim;
    }

    /**
     * @param mixed $data
     */
    public function setData($data)
    {
        $this->data = $data;
    }

    /**
     * @return mixed
     */
    public function getData()
    {
        return $this->data;
    }

    /**
     * @param mixed $hora_inicio
     */
    public function setHoraInicio($hora_inicio)
    {
        $this->hora_inicio = $hora_inicio;
    }

    /**
     * @return mixed
     */
    public function getHoraInicio()
    {
        return $this->hora_inicio;
    }

    /**
     * @param mixed $id
     */
    public function setId($id)
    {
        $this->id = $id;
    }

    /**
     * @return mixed
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * @param mixed $id_destino
     */
    public function setIdDestino($id_destino)
    {
        $this->id_destino = $id_destino;
    }

    /**
     * @return mixed
     */
    public function getIdDestino()
    {
        return $this->id_destino;
    }

    /**
     * @param mixed $id_dono
     */
    public function setIdDono($id_dono)
    {
        $this->id_dono = $id_dono;
    }

    /**
     * @return mixed
     */
    public function getIdDono()
    {
        return $this->id_dono;
    }

    /**
     * @param mixed $local
     */
    public function setLocal($local)
    {
        $this->local = $local;
    }

    /**
     * @return mixed
     */
    public function getLocal()
    {
        return $this->local;
    }

    /**
     * @param mixed $nome
     */
    public function setNome($nome)
    {
        $this->nome = $nome;
    }

    /**
     * @return mixed
     */
    public function getNome()
    {
        return $this->nome;
    }

    /**
     * @param mixed $tipo_destino
     */
    public function setTipoDestino($tipo_destino)
    {
        $this->tipo_destino = $tipo_destino;
    }

    /**
     * @return mixed
     */
    public function getTipoDestino()
    {
        return $this->tipo_destino;
    }

    /**
     * @param mixed $tipo_dono
     */
    public function setTipoDono($tipo_dono)
    {
        $this->tipo_dono = $tipo_dono;
    }

    /**
     * @return mixed
     */
    public function getTipoDono()
    {
        return $this->tipo_dono;
    }


}


